@font-face
    font-family:"Fira Logic"
    src:url("https://cdn.jsdelivr.net/gh/gleachkr/FiraLogic/dist/FiraLogic-Regular.woff") format("woff"), 
        url("https://cdn.jsdelivr.net/gh/gleachkr/FiraLogic/dist/FiraLogic-Regular.tff") format("tff"),
        url("https://cdn.jsdelivr.net/gh/gleachkr/FiraLogic/dist/FiraLogic-Regular.otf") format("otf")
        

h1,h2,h3,h4
    font-family:Fira Sans

p, ul, ol, dd, .text, footer
    font-family:Open Sans

.proofFeedback li
    line-height: 1.2 em

pre
    overflow: visible
    font-family:Fira Logic

pre *
    font-family:Fira Logic !important
    white-space:pre-wrap
    white-space: -moz-pre-wrap
